{
  "manifest_version": 3,
  "name": "Readhub.Tab",
  "description": "Readhub 新标签页插件",
  "version": "1.4.0",
  "incognito": "split",
  "chrome_url_overrides":
  {
    "newtab": "index.html"
  },
  "permissions": ["https://api.readhub.cn/*"],
  "icons":
  {
    "16": "static/icon16.png",
    "48": "static/icon48.png",
    "128": "static/icon128.png",
    "256": "static/icon256.png"
  }
}
