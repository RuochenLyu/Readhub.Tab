{
  "manifest_version": 2,
  "name": "Readhub.Tab",
  "description": "Readhub.me 新标签页插件",
  "version": "1.2.4",
  "incognito": "split",
  "chrome_url_overrides":
  {
    "newtab": "index.html"
  },
  "permissions": ["storage", "https://api.readhub.me/*"],
  "icons":
  {
    "16": "static/icon16.png",
    "48": "static/icon48.png",
    "128": "static/icon128.png",
    "256": "static/icon256.png"
  }
}
