{
  "manifest_version": 2,
  "name": "Readhub.Tab",
  "description": "Readhub New Tab",
  "version": "1.1.0",
  "incognito": "split",
  "chrome_url_overrides":
  {
    "newtab": "index.html"
  },
  "permissions": ["storage", "http://api.rss2json.com/*"],
  "icons":
  {
    "16": "static/icon16.png",
    "48": "static/icon48.png",
    "128": "static/icon128.png",
    "256": "static/icon256.png"
  }
}
